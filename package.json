        ],
        "*.ts?(x)": [
            "prettier --parser=typescript --write"
        ]
    },
    "dependencies": {
        "@umijs/preset-react": "1.x",
        "@umijs/test": "^3.2.0-beta.6",
        "lint-staged": "^10.0.7",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "styled-components": "^5.1.0",
        "umi": "^3.2.0-beta.6",
        "yorkie": "^2.0.0"
    },
    "devDependencies": {
        "@types/styled-components": "^5.1.0"
    }
}
